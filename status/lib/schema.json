{
  "dynamic" : false,

  "dynamic_templates": [
    {
      "labels": {
        "match_mapping_type": "string",
        "match":   "*.status",
        "mapping": {
          "type" : "keyword"
        }
      }
    }
  ],

  "properties" : {
    
    "id" : {
      "type" : "keyword"
    },

    "shortId" : {
      "type" : "keyword",
      "fields": {
        "text" : {
          "type" : "text",
          "analyzer": "autocomplete", 
          "search_analyzer": "autocomplete_search"
        }
      }
    }

  }
}